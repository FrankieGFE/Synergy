SELECT DISTINCT

        org.ORGANIZATION_NAME    AS [Institution]
      , stf.BADGE_NUM            AS [Primary Staff ID]
      , ''                       AS [Secondary IDs]
      , stfp.LAST_NAME           AS [Last Name]
      , stfp.FIRST_NAME          AS [First Name]
      , 'Standard'               AS [Access Privileges]
      , 'Teacher'                AS [User Type]
