	         WHEN org.ORGANIZATION_NAME = 'Acoma Elementary School'                  THEN 'K3' + stu.STATE_STUDENT_NUMBER
			 WHEN org.ORGANIZATION_NAME = 'Alvarado Elementary School'               THEN 'K3' + stu.STATE_STUDENT_NUMBER
