      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E5FACB16-9082-4240-8AC6-EB883CDD3458}"
          SQLTask:SqlStatementSource="IF OBJECT_ID('dbo.CleanName') IS NOT NULL DROP FUNCTION dbo.CleanName&#xA;GO&#xA;--function to cleanup name string&#xA;Create Function dbo.CleanName(@Temp VarChar(1000))&#xA;Returns VarChar(1000)&#xA;AS&#xA;Begin&#xA;    Declare @CleanValues as varchar(50)&#xA;    Set @CleanValues = '%[^a-z ]%'&#xA;    While PatIndex(@CleanValues, @Temp) &gt; 0&#xA;        Set @Temp = Stuff(@Temp, PatIndex(@CleanValues, @Temp), 1, '')&#xA;    Return @Temp&#xA;End&#xA;go" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
