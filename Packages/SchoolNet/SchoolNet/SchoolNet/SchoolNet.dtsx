                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--&lt;APS - SchoolNet student_contact data&gt;

SELECT DISTINCT
         stu.SIS_NUMBER + pper.FIRST_NAME               AS [student_contact_code]
       , pper.LAST_NAME                                 AS [last_name]
       , pper.FIRST_NAME                                AS [first_name]
       , pper.MIDDLE_NAME                               AS [middle_name]
       , ''                                             AS [race_ethnicity_code]
       , pper.GENDER                                    AS [gender_code]
       , ''                                             AS [dob]
       , hadr.ADDRESS                                   AS [address_1]
       , hadr.ADDRESS2                                  AS [address_2]
       , hadr.CITY                                      AS [city]
       , hadr.STATE                                     AS [state]
       , hadr.ZIP_5 + COALESCE(hadr.zip_4, '0000')      AS [zip]
       , LEFT(pper.PRIMARY_PHONE,3)
	     + '-' + SUBSTRING(pper.PRIMARY_PHONE,4,3)
		 + '-' + RIGHT(pper.PRIMARY_PHONE,4)            AS [phone]
       , pper.EMAIL                                     AS [email]
FROM   rev.EPC_STU                    stu
       JOIN rev.EPC_STU_SCH_YR        ssy  ON ssy.STUDENT_GU = stu.STUDENT_GU
       JOIN rev.REV_ORGANIZATION_YEAR oyr  ON oyr.ORGANIZATION_YEAR_GU = ssy.ORGANIZATION_YEAR_GU
                                              and oyr.YEAR_GU = (select YEAR_GU from rev.SIF_22_Common_CurrentYearGU)
       JOIN rev.REV_YEAR              yr   ON yr.YEAR_GU          = oyr.YEAR_GU
       JOIN rev.EPC_STU_PARENT        spar ON spar.STUDENT_GU = stu.STUDENT_GU
       JOIN rev.REV_PERSON            pper ON pper.PERSON_GU  = spar.PARENT_GU
	   JOIN rev.REV_ADDRESS           hadr ON hadr.ADDRESS_GU = pper.HOME_ADDRESS_GU</property>
