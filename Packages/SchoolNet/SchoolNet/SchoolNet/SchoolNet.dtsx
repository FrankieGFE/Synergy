                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--&lt;APS - SchoolNet code_withdrawl_type data&gt;
declare @vSchYr varchar(4)
set @vSchYr = cast((select school_year from rev.SIF_22_Common_CurrentYear) as varchar(4))

SELECT
        @vSchYr + '-' + SUBSTRING(wcd.VALUE_DESCRIPTION,1,2) AS [ withdrawal_type_code]
      , wcd.VALUE_DESCRIPTION                                AS [withdrawal_type_name]
FROM  (select 
