Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	
    Public Sub Main()
        Dim fileLoc, fileName As String
        If Dts.Variables.Contains("User::LibraryHomeroomFilePath") = True AndAlso _
        Dts.Variables.Contains("User::LibraryHomeroomFile") = True Then
            fileLoc = CStr(Dts.Variables("User::LibraryHomeroomFilePath").Value)
            fileName = CStr(Dts.Variables.Item("User::LibraryHomeroomFile").Value)
            If File.Exists(fileLoc + fileName) Then
                Dts.Variables.Item("User::FileExists").Value = True
            Else
                Dts.Variables.Item("User::FileExists").Value = False
            End If
            Dts.TaskResult = ScriptResults.Success
        Else
            Dts.TaskResult = ScriptResults.Failure
        End If
    End Sub

End Class]]></ProjectItem><ProjectItem Name="\my project\resources.resx">
