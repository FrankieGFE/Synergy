       from  rev.REV_BOD_LOOKUP_VALUES lv 
       where LOOKUP_DEF_GU in
             (select LOOKUP_DEF_GU 
	          from   rev.REV_BOD_LOOKUP_DEF
              where  LOOKUP_NAMESPACE = 'K12.Enrollment' and LOOKUP_DEF_CODE = 'LEAVE_CODE')
