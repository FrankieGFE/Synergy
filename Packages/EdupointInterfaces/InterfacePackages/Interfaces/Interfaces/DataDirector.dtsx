              description="OLE DB Source"
              name="SMAXODS Lawson"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--SELECT * FROM [APS].[SynergyDataDirectorUser]

SELECT DISTINCT
	[Lawson_Staff].[STAFF_CODE]
	,[Lawson_Staff].LOCATION
	,[Lawson_Staff].[FIRST_NAME]
	,[Lawson_Staff].[LAST_NAME]
	,[Lawson_Staff].[MIDDLE_NAME]
	,[Lawson_Staff].[EMAIL]
	,[Lawson_Staff].[PASSWORD]
	,[Lawson_Staff].[USERNAME]
FROM
	(	
		
	SELECT 
		EMPLOYEE.EMPLOYEE AS STAFF_CODE 
		--for RDA employees change location to a 1, for Summer Employees use the USER LEVEL
		,CASE WHEN EMPLOYEE.USER_LEVEL = '0046' THEN '1' 
			  WHEN LEFT(ASSIGNMENTS.POSITION,3) = '513%' THEN ASSIGNMENTS.USER_LEVEL
		ELSE LEFT(EMPLOYEE.POSITION,3) END AS LOCATION
		
		,FIRST_NAME
		,LAST_NAME
		,MIDDLE_NAME
		,EMAIL_ADDRESS AS EMAIL
		,'TCHTEST2' AS [PASSWORD]
		,EMPLOYEE.EMPLOYEE AS USERNAME
		
	FROM
		--read Active Lawson employes, include employees on leave, not terminated or retired
		[Lawson].dbo.Employee AS EMPLOYEE	
	
		LEFT JOIN
			Lawson.dbo.Paemppos AS ASSIGNMENTS
			ON
			EMPLOYEE.EMPLOYEE = ASSIGNMENTS.EMPLOYEE
			AND ASSIGNMENTS.END_DATE IS NULL
			AND ASSIGNMENTS.POSITION LIKE '513%'
			
	WHERE
		--this gets rid of employees that work at M&amp;O, cafteria, and custodians
		SUBSTRING(EMPLOYEE.POSITION,4,5)NOT IN ('CT', 'D CUS', 'D MAI', 'D OPT', 'D SRT', 'D STR', 'D YRD', 'C CAF')
		--this includes Brianna Beitler which is an inactive employee and excludes retired/terminated employees
		AND	(
			EMPLOYEE.EMPLOYEE = 122405 OR 
			EMP_STATUS NOT IN ('R1', 'R2','T2', 'T1', 'ZZ')
			)
	--This inserts 3 records needed for Data Director test admin purposes
	UNION
		SELECT 
			'99999998','1','HEATHER','PILLMAN','','Heather.Pillman@hmhpub.com','TCHTEST2','99999998'
	UNION
		SELECT 		
			'100030175','1','TEST ADMIN','TEST ADMIN','','','TCHTEST2','100030175'
	UNION
		SELECT 			
			'100030176','1','TEST ADMIN','TEST ADMIN','','','TCHTEST2','100030176'
	) AS [Lawson_Staff]
	
	
LEFT JOIN
		(SELECT SCHOOL_CODE 
			FROM [SYNERGYDBDC.APS.EDU.ACTD] .ST_Production.rev.REV_ORGANIZATION AS ORG
			INNER JOIN
			[SYNERGYDBDC.APS.EDU.ACTD] .ST_Production.rev.EPC_SCH AS SCH
			ON ORG.ORGANIZATION_GU = SCH.ORGANIZATION_GU
		)AS SYNERGYLOCATIONS
		
		ON
		SYNERGYLOCATIONS.SCHOOL_CODE = Lawson_Staff.LOCATION
		AND Lawson_Staff.LOCATION != '1'


WHERE
	SYNERGYLOCATIONS.SCHOOL_CODE IS NOT NULL
	OR LOCATION = '1'</property>
