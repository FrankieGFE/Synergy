SELECT DISTINCT

        org.ORGANIZATION_NAME    AS [Institution]
      , stf.BADGE_NUM            AS [Primary Staff ID]
      , ''                       AS [Secondary IDs]
      , stfp.LAST_NAME           AS [Last Name]
      , stfp.FIRST_NAME          AS [First Name]
      , 'Standard'               AS [Access Privileges]
      , 'Teacher'                AS [User Type]
      , 'Item Access'            AS [Beacon Item Pool Role]
      , 'Default'                AS [Beacon Standards Role]
      , stfp.email               AS [Email]

FROM  rev.EPC_STU                         stu
      JOIN rev.EPC_STU_SCH_YR             ssy   ON ssy.STUDENT_GU              = stu.STUDENT_GU
                                                   AND ssy.STATUS is NULL
      JOIN rev.REV_ORGANIZATION_YEAR      oyr   ON oyr.ORGANIZATION_YEAR_GU    = ssy.ORGANIZATION_YEAR_GU
      JOIN rev.REV_YEAR                   yr    ON yr.YEAR_GU                  = oyr.YEAR_GU
                                                   AND (yr.YEAR_GU IN (SELECT YEAR_GU FROM APS.YearDates WHERE GETDATE() BETWEEN YearDates.START_DATE AND YearDates.END_DATE)
