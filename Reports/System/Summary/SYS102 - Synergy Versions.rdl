FROM 
    [ST_Release].[rev].[REV_VER]

ORDER BY
    [DEPLOYMENT_DATE] DESC
) AS [Release]
UNION ALL
SELECT 
    *
FROM
(
    SELECT TOP 1
    'SPED' AS [Environment]
    ,[VERSION]
FROM 
    [ST_SPED].[rev].[REV_VER]

ORDER BY
    [DEPLOYMENT_DATE] DESC
) AS [SPED]

UNION ALL
SELECT 
    *
FROM
(
    SELECT TOP 1
    'Stars' AS [Environment]
    ,[VERSION]
FROM 
    [ST_Stars].[rev].[REV_VER]

ORDER BY
    [DEPLOYMENT_DATE] DESC
) AS [Stars]

UNION ALL
SELECT 
    *
FROM
(
    SELECT TOP 1
    'Training' AS [Environment]
    ,[VERSION]
FROM 
    [ST_Train].[rev].[REV_VER]

ORDER BY
    [DEPLOYMENT_DATE] DESC
) AS [Stars]</CommandText>
      </Query>
