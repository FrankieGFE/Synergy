  <DataSets>
    <DataSet Name="Production_Version">
      <Query>
        <DataSourceName>dsSynergyDBDC</DataSourceName>
        <CommandText>SELECT TOP 1 
	'Live' AS [Environment]
	,[VERSION]
  FROM [ST_Production].[rev].[REV_VER]

ORDER BY
    [DEPLOYMENT_DATE] DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="VERSION">
          <DataField>VERSION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Environments_Versions">
      <Query>
        <DataSourceName>dsSynSecondDB</DataSourceName>
        <CommandText>SELECT 
    *
FROM
(
    SELECT TOP 1 
    'Experiment' AS [Environment]
    ,[VERSION]
FROM 
    [ST_Experiment].[rev].[REV_VER]

ORDER BY
    [DEPLOYMENT_DATE] DESC
) AS [Exp]

UNION ALL
SELECT 
    *
FROM
(
    SELECT TOP 1
    'Functional' AS [Environment]
