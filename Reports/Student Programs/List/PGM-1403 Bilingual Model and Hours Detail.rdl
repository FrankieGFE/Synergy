    <DataSet Name="BilingualWithDetails">
      <Query>
        <DataSourceName>Synergy</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@asOfDate">
            <Value>=Parameters!AsOfDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@School">
            <Value>=Parameters!School.Value</Value>
